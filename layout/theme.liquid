  {%if template contains "product" %}
  <script text="text/javascript">
    var _learnq = _learnq || [];

    var item = {
      Name: {{ product.title|json }},
      ProductID: {{ product.selected_or_first_available_variant.id}},
      Categories: {{ product.collections|map:'title'|json }},
      ImageURL: "https:{{ product.featured_image.src|img_url:'grande' }}",
      URL: "{{ shop.secure_url }}{{ product.url }}",
      Brand: {{ product.vendor|json }},
      Price: "${% if product.selected_or_first_available_variant.metafields.global.dkprice-us-sale %}{{product.selected_or_first_available_variant.metafields.global.dkprice-us-sale}}{%else%}{{product.selected_or_first_available_variant.metafields.global.dkprice-us}}{%endif%}",
      CompareAtPrice: "${{product.selected_or_first_available_variant.metafields.global.dkprice-us}}"
    };

    _learnq.push(['track', 'Viewed Product', item]);
    _learnq.push(['trackViewedItem', {
      Title: item.Name,
      ItemId: item.ProductID,
      Categories: item.Categories,
      ImageUrl: item.ImageURL,
      Url: item.URL,
      Metadata: {
        Brand: item.Brand,
        Price: item.Price,
        CompareAtPrice: item.CompareAtPrice
      }
    }]);
  </script>

  <script type="text/javascript">
    var _learnq = _learnq || [];
    document.getElementById("AddToCart-{{ product.id }}").addEventListener('click',function (){
                            _learnq.push(['track', 'Added to Cart', item]);
    });
  </script>
